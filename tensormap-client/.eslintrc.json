{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "react-app",
        "react-app/jest",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "object-curly-spacing": "off",
        "object-curly-newline": "off",
        "quotes": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-indent": "off",
        "react/jsx-filename-extension": "off",
        "comma-spacing": "off",
        "jsx-quotes": "off"
    }
}
